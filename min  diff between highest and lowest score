class Solution:
    def minimumDifference(self, nums: List[int], k: int) -> int:
        n=len(nums)
        score=float('inf')
        if k==0:
            return 0
        nums.sort()
        for i in range(n-k+1):
            diff = nums[i + k - 1] - nums[i]
            score = min(score, diff)

        return score
